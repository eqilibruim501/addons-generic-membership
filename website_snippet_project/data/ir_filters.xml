<odoo>
    <!-- Filters for Dynamic Filter -->
    <record id="dynamic_snippet_projects" model="ir.filters">
        <field name="name">Projects</field>
        <field name="model_id">project.project</field>
        <field name="user_id" eval="False" />
        <field name="domain">[('is_published', '=', True)]</field>
        <field name="sort">['name asc']</field>
        <field name="action_id" ref="website.action_website"/>
    </record>
</odoo>

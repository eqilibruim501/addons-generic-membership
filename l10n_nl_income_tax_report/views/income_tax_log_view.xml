<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="income_tax_log_tree" model="ir.ui.view">
        <field name="model">l10n.nl.income.tax.log</field>
        <field name="arch" type="xml">
            <tree>
                <field name="currency_id" invisible="1" />
                <field name="rule_id" />
                <field name="taxable_income" />
                <field name="deduction" />
                <field name="tax" />
            </tree>
        </field>
    </record>

    <record id="income_tax_log_form" model="ir.ui.view">
        <field name="model">l10n.nl.income.tax.log</field>
        <field name="arch" type="xml">
            <form>
                <field name="currency_id" invisible="1" />
                <group>
                    <field name="taxable_income" />
                    <field name="deduction" />
                    <field name="tax" />
                    <field name="description" />
                </group>
            </form>
        </field>
    </record>
</odoo>

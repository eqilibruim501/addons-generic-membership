<?xml version="1.0" encoding="UTF-8" ?>
<odoo noupdate="1">
    <record id="parameter_url" model="ir.config_parameter">
        <field name="key">base_librecaptcha.url</field>
        <field name="value"></field>
    </record>

    <record id="parameter_level" model="ir.config_parameter">
        <field name="key">base_librecaptcha.level</field>
        <field name="value">hard</field>
    </record>

    <record id="parameter_media" model="ir.config_parameter">
        <field name="key">base_librecaptcha.media</field>
        <field name="value">image/gif</field>
    </record>
</odoo>
